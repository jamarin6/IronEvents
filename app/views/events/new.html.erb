<h1>Events#new</h1>
<p>Find me in app/views/events/new.html.erb</p>


<%= form_for @event do |f| %>

	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :description %>
	<%= f.text_field :description %>

	<%= f.label :address %>
	<%= f.text_field :address %>

	<%= f.label :start_at %>
	<%= f.date_field :start_at %>

	<%= f.label :end_at %>
	<%= f.date_field :end_at %>

	<hr>
	<%= f.submit "Create new" %>
<% end %>