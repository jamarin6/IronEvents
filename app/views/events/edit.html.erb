<h1>Events#edit</h1>
<p>Find me in app/views/events/edit.html.erb</p>

<%= form_for @event do |f| %>

	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :description %>
	<%= f.text_field :description %>
	<%= f.label :address %>
	<%= f.text_field :address %>
	<%= f.label :start_at %>
	<%= f.text_field :start_at %>
	<%= f.label :end_at %>
	<%= f.text_field :end_at %>

	<hr>
	<%= f.submit "Edit" %>
<% end %>
